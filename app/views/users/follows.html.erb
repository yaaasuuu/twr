<div class="main">
    <div class="memList">
        <% @follows.each  do |follow|%>
        <% users = User.find_by(id: follow.at_user_id)%>
        <img src="<%="/user_images/#{users.image_name}"%>" alt="IMG" width="100" height="100">
        <p class="userName"><%= link_to(users.name,"/users/#{users.id}")%></p>
        <p class="userID"><a>@</a><%= users.account_name%></p>
        <%end%>
    </div>
</div>